<h2 class="col-12 text-center text-md-start">
    Mon compte
</h2>
<ul class="list-unstyled text-center text-md-start">    
  {% for theme in themes %}
    {% if is_granted(theme.role_min) %}
      <li class="mt-3"><img src="{{ asset('build/images/design/member/miniLogos/' ~ theme.imgName) }}"><a class="text-decoration-none text-dark" href="{{ theme.link }}"> {{ theme.title }}</a></li>
    {% endif %}
  {% endfor %}  
</ul>